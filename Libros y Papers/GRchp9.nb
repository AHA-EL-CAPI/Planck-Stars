Cell[TextData[{
  "Curvature tensor  ",
  Cell[BoxData[
      \(TraditionalForm\`R\_\[Beta]\[Gamma]\[Delta]\%\[Alpha]\)]],
  "   "
}], "Subsubsection"],

Cell["\<\
curvR[ch_,var_:{t,r,\[Theta],\[Phi]}] :=
   Module[{temp1},
   temp1= Outer[D,ch,var];   
  (   Transpose[Outer[D,ch,var],{ 1,2,4 ,3}]-
      Transpose[Outer[D,ch,var ],{1,2,3,4}]+
      Transpose[Inner[Times,ch,ch,Plus,2 ],
\t\t                         {1,3,4,2}]-
      Transpose[Inner[Times,ch,ch,Plus,2 ],
\t\t                         {1,4,3,2}] )
                                       ]   \
\>", "Input"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
  "NonZcurv",
  Cell[BoxData[
      \(TraditionalForm\`\(\(\ \ \)\(R\_\[Alpha]\[Beta]\[Gamma]\[Delta]\%\ \)\
\)\)]],
  " "
}], "Subsubsection"],

Cell["\<\
 NonZcurv[name_,curvR_,met_,var_:{t,r,\[Theta],\[Phi]}] :=
  Module[
   {temp1,temp2,temp3,temp4,temp5,R,i,j,k,s,l, A$,B$,C1$},    
temp1 =met.curvR; 
temp2[i_,j_,k_,l_]:= temp1[[i,j,k,l]];  
temp3= {R[1,2,1,2],R[1,2,1,3],R[1,2,1,4], 
        R[1,2,2,3],R[1,2,2,4],R[1,2,3,4],
        R[1,3,1,3],R[1,3,1,4],R[1,3,2,3],
        R[1,3,2,4],R[1,3,3,4],R[1,4,1,4],
        R[1,4,2,3],R[1,4,2,4],R[1,4,3,4],
        R[2,3,2,3],R[2,3,2,4],R[2,3,3,4],
        R[2,4,2,4],R[2,4,3,4],R[3,4,3,4]}  ;       
temp4= temp3/.R->name/.(#->var[[#]]&/@Range[4]);  
temp5= (temp4)->(temp3/.R->temp2)//Thread;
(temp5//Simplify)//.{A$___,B$_->0,C1$___}->
                                {A$,C1$}]  \
\>", "Input"]
}, Open  ]],

