Cell[TextData[{
  "Ricci Tensor  ",
  Cell[BoxData[
      \(TraditionalForm\`R\_\[Alpha]\[Beta]\)]],
  "   "
}], "Text"],

Cell["\<\
 ricciR[curv_,dem_:4]:= 
  Simplify[ExpandAll[Table[
  Sum[curv[[s,i,s,j]],{s,dem}],{i,dem},{j,dem}]]]\
\>", "Input"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
  "Weyl Curvature ",
  Cell[BoxData[
      \(TraditionalForm\`C\_\[Alpha]\[Beta]\[Gamma]\[Delta]\)]]
}], "Text"],

Cell["\<\
 curvW[met_,cur_]:=
 Module[{temp1,temp2,temp3,temp4,temp5,temp6,i},    
  temp1= ricciR[cur];
  temp2= Sum[(Inverse[met].temp1 )[[i,i]],{i,4}];
  temp3= Outer[Times,met,temp1] ;
  temp4=  Transpose[temp3,{1,4,3,2}]+ 
               Transpose[temp3,{2,3,4,1}]- 
                    Transpose[temp3,{1,3,4,2}]-
                         Transpose[temp3,{2,4,3,1}];
  temp5= Outer[Times,met,met] ; 
  temp6= Transpose[temp5,{1,3,4,2}]- 
                           Transpose[temp5,{1,4,3,2}]; 
   met.cur +1/2 temp4 +1/6 temp6 temp2]
                     
            
         \
\>", "Input"]
}, Open  ]]
}, Open  ]],

